{
"type": "object",
"$id": "schema:dotenv", 
"required": ["MONGO_URL"],
"properties": {
  "NODE_ENV": {
      "type": "string",
      "default": "development"
    }, 
  "PORT": {
      "type": "integer",
      "default": 3000
    },
  "JWT_SECRET": {
      "type": "string",
      "default": "changethis"
    },
  "JWT_EXPIRE_IN": {
      "type": "integer",
      "default": 3000
    },
  "MONGO_URL": {
      "type": "string"
    }
  }
}